{
	"name": "arsaey/persian",
	"type": "flarum-extension",
	"description": "Persian language pack for Flarum. (shamsi date)",
	"homepage": "https://github.com/flarum-lang/persian",
	"keywords": [
		"locale",
		"flarum",
		"persian"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/persian/issues",
		"source": "https://github.com/flarum-lang/persian"
	},
	"require": {
		"flarum/core": "^1.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Persian (shamsi)",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "fa",
			"title": "Persian"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
